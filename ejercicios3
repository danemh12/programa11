#include<stdio.h>

main(){
	
	int y;
	
	y=5/2+20%6;
	printf("%d\n", y);
	y=4*6/2-15/2;
	printf("%d\n",y);
	y=5*15/2/(4-2);
	printf("%d\n", y);
	y=8==16||7!=4&&4<1;
	printf("%d\n", y);
	y=(4*3<6||3>5-2)&&3+2<12;
	printf("%d\n", y);
	y=7+3*6/2-1;
	printf("%d\n",y);
	y=2%2+2*2-2/2;
	printf("%d\n", y);
	y=(3*9*(3+(9*3/(3))));
	printf("%d\n", y);
	//el resultado es 4, 5, 18, 0. 0, 15, 3, 324
}
